{
  "family": "java-app",
  "networkMode": "awsvpc", 
  "executionRoleArn": "%ECS_TASKEXEC_ARN%",
  "containerDefinitions": [{
    "name": "java-app",
    "essential": true,
    "image": "%ECR_IMAGE%",
    "memoryReservation": 512,
    "portMappings": [
      {
        "protocol": "tcp",
        "containerPort": 5000,
        "hostPort": 5000
      }
    ]
  }],
  "requiresCompatibilities": [
        "FARGATE"
  ], 
  "cpu": "256", 
  "memory": "1024"
}